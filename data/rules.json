      "start_register": "0",
      "end_register": "1000"
    },
    {
      "command": "Read Holding Registers",
      "id": 1004,
      "is_active": "true",
      "name": "Accept MODBUS TCP reads from 205 to 220 on single registers",
      "protocol": "MODBUS",
      "start_register": "205",
      "end_register": "220"
    },
    {
      "command": "Read",
      "subcommand": "Read from bit dev in 16 point units",
      "id": 2001,
      "is_active": "true",
      "name": "Allow SLMP reads from 0 to 20 registers",
      "protocol": "SLMP",
      "start_register": "0",
      "end_register": "20"
    }
  ]
