      "name": "Allow all TCP traffic from 192.168.2.2 to 192.168.1.2",
      "protocol": "TCP",
      "source_address": "192.168.2.2",
      "source_port": "ANY"
    },
    {
      "destination_address": "192.168.2.2",
      "destination_port": "ANY",
      "id": 1002,
      "is_active": "true",
      "name": "Allow all TCP traffic from 192.168.1.2 to 192.168.2.2",
      "protocol": "TCP",
      "source_address": "192.168.1.2",
      "source_port": "ANY"
    },
    {
      "command": "Write Multiple Registers",
      "id": 1003,
      "is_active": "true",
      "name": "Accept MODBUS TCP writes from 0 to 1000 on single registers",
